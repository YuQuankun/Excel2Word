                                <w:shd w:val="pct10" w:color="auto" w:fill="FFFFFF"/>
                            </w:rPr>
                            <w:t>38、50/50条款</w:t>
                        </w:r>
                    </w:p>
                    <w:p w14:paraId="399FFB29" w14:textId="77777777" w:rsidR="00F151AD" w:rsidRDefault="00000000">
                        <w:pPr>
                            <w:adjustRightInd w:val="0"/>
