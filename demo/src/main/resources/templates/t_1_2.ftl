                            <w:t>（七）领取公共行驶执照的机动车辆；</w:t>
                        </w:r>
                    </w:p>
                    <w:p w14:paraId="7D00ACB5" w14:textId="77777777" w:rsidR="00F151AD" w:rsidRDefault="00000000">
                        <w:pPr>
                            <w:snapToGrid w:val="0"/>
