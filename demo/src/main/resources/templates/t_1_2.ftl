                                <w:szCs w:val="24"/>
                            </w:rPr>
                            <w:t>（七）盘点时发现的短缺；</w:t>
                        </w:r>
                    </w:p>
                    <w:p w14:paraId="432CB219" w14:textId="77777777" w:rsidR="00F151AD" w:rsidRDefault="00000000">
