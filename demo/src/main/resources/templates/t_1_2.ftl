                                <w:szCs w:val="24"/>
                            </w:rPr>
                            <w:t>限额：每次事故损失金额的10％。</w:t>
                        </w:r>
                    </w:p>
                    <w:p w14:paraId="116F90D1" w14:textId="77777777" w:rsidR="00F151AD" w:rsidRDefault="00000000">
                        <w:pPr>
                            <w:adjustRightInd w:val="0"/>
