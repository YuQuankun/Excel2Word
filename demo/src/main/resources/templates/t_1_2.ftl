                            <w:rPr>
                                <w:rFonts w:ascii="方正仿宋_GBK" w:eastAsia="方正仿宋_GBK" w:hAnsi="方正仿宋_GBK" w:cs="方正仿宋_GBK"
                                          w:hint="default"/>
                                <w:sz w:val="24"/>
                                <w:szCs w:val="24"/>
                            </w:rPr>
                        </w:pPr>
                        <w:r>
                            <w:rPr>
                                <w:rFonts w:ascii="方正仿宋_GBK" w:eastAsia="方正仿宋_GBK" w:hAnsi="方正仿宋_GBK" w:cs="方正仿宋_GBK"/>
                                <w:sz w:val="24"/>
                                <w:szCs w:val="24"/>
                            </w:rPr>
                            <w:lastRenderedPageBreak/>
                            <w:t>（九）本保险合同中载明的免赔额或按本保险合同中载明的免赔率计算的免赔额。</w:t>
                        </w:r>
                    </w:p>
