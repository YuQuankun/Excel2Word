                                <w:sz w:val="24"/>
                                <w:szCs w:val="24"/>
                            </w:rPr>
                            <w:t>（五）非外力造成机械或电气设备本身的损失；</w:t>
                        </w:r>
                    </w:p>
                    <w:p w14:paraId="4B8AFA6E" w14:textId="77777777" w:rsidR="00F151AD" w:rsidRDefault="00000000">
