                                <w:rFonts w:ascii="方正仿宋_GBK" w:eastAsia="方正仿宋_GBK" w:hAnsi="方正仿宋_GBK" w:cs="方正仿宋_GBK"
                                          w:hint="default"/>
                                <w:sz w:val="24"/>
                                <w:szCs w:val="24"/>
                            </w:rPr>
                        </w:pPr>
                        <w:r>
                            <w:rPr>
                                <w:rFonts w:ascii="方正仿宋_GBK" w:eastAsia="方正仿宋_GBK" w:hAnsi="方正仿宋_GBK" w:cs="方正仿宋_GBK"/>
                                <w:sz w:val="24"/>
                                <w:szCs w:val="24"/>
                            </w:rPr>
                            <w:t>本附加条款项下保险人的赔偿责任不超过本保险合同中载明的相应赔偿限额。</w:t>
                        </w:r>
                    </w:p>
                    <w:p w14:paraId="676B10C7" w14:textId="77777777" w:rsidR="00F151AD" w:rsidRDefault="00000000">
                        <w:pPr>
