                        </w:pPr>
                        <w:r>
                            <w:rPr>
                                <w:rFonts w:ascii="方正仿宋_GBK" w:eastAsia="方正仿宋_GBK" w:hAnsi="方正仿宋_GBK" w:cs="方正仿宋_GBK"/>
                                <w:sz w:val="24"/>
                                <w:szCs w:val="24"/>
                            </w:rPr>
                            <w:t>（八）任何原因导致公共供电、供水、供气及其他能源供应中断造成的损失和费用；</w:t>
                        </w:r>
                    </w:p>
                    <w:p w14:paraId="24B93E34" w14:textId="77777777" w:rsidR="00F151AD" w:rsidRDefault="00000000">
                        <w:pPr>
                            <w:snapToGrid w:val="0"/>
