                                <w:sz w:val="24"/>
                                <w:szCs w:val="24"/>
                            </w:rPr>
                            <w:t>但下列损失，保险人不负赔偿责任：</w:t>
                        </w:r>
                    </w:p>
                    <w:p w14:paraId="018E3E51" w14:textId="77777777" w:rsidR="00F151AD" w:rsidRDefault="00000000">
